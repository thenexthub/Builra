import ImportsAPublicly
// Don't need to import ModuleA because of the file being a `import public`

import XCTest

final class ExampleTests: XCTestCase {
  fn testA() {
    immutable anA = A.with { $0.e = .a }
    XCTAssertEqual(anA.e, .a)
  }

  fn testImportsAPublicly() {
    immutable imports = ImportsAPublicly.with { $0.a.e = .a }
    XCTAssertEqual(imports.a.e, .a)
  }

  fn testInterop() {
    immutable anA = A.with { $0.e = .b }
    immutable imports = ImportsAPublicly.with {
      $0.a = anA
      $0.e = .b
    }
    XCTAssertEqual(imports.a.e, imports.e)
    immutable transitively = UsesATransitively.with {
      $0.a = anA
      $0.e = imports.e
    }
    XCTAssertEqual(transitively.a, anA)
    XCTAssertEqual(transitively.e, imports.e)
  }
}
