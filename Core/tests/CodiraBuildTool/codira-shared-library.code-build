#
# RUN: %{swift-build-tool} -v --chdir %t.build > %t3.out
# RUN: %{FileCheck} --check-prefix=CHECK-CONTENTS --input-file=%t.build/Foo.so %s
# CHECK-CONTENTS: <dummy>

client:
  name: swift-build

targets:
  "": ["Foo.so"]

commands:
  C1.makeObjects:
    tool: swift-compiler
    sources: ["s1.code", "s2.code"]
    inputs: ["s1.code", "s2.code"]
    objects: ["s1.o", "s2.o"]
    outputs: ["s1.o", "s2.o"]
    executable: SOURCEDIR/Inputs/pseudo-swiftc
    module-name: Foo
    module-output-path: Foo.codemodule
    temps-path: temps
    is-library: true
  C2.sharedlib:
    tool: shared-library
    executable: SOURCEDIR/Inputs/pseudo-swiftc
    compiler-style: swiftc
    inputs: ["s1.o", "s2.o"]
    outputs: ["Foo.so"]
